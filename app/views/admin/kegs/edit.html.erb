<div class="page-header">
  <h1>Edit Keg: <%= @keg.name %></h1>
</div>

<%= errors_for(@keg) %>

<%= form_for [:admin, @keg] do |f| %>
  <%= render 'form', {f: f} %>

  <div class="form-actions">
    <%= f.submit 'Update', :class => 'btn btn-primary' %>
    <%= link_to 'Cancel', [:admin, @keg], :class => 'btn' %>
  </div>
<% end %>
