<div class="page-header">
  <h1>New Keg</h1>
</div>

<%= errors_for(@keg) %>

<%= form_for [:admin, @keg] do |f| %>
  <%= render 'form', {f: f} %>

  <div class="form-actions">
    <%= f.submit 'Create', :class => 'btn btn-primary' %>
    <%= link_to 'Cancel', admin_kegs_path, :class => 'btn btn-default' %>
  </div>
<% end %>
